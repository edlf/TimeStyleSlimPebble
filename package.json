{
  "name": "timestyle-pebble-slim",
  "author": "Freakified, edlf",
  "version": "1.0.0",
  "keywords": [
    "pebble-watchface"
  ],
  "private": true,
  "dependencies": {
    "pebble-fctx": "^1.4.0"
  },
  "pebble": {
    "displayName": "TimeStyleSlim",
    "uuid": "CA43E11F-47C9-4263-AF0D-F3D2116B6C1C",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "basalt",
      "chalk",
      "diorite",
      "emery"
    ],
    "capabilities": [
      "configurable"
    ],
    "watchapp": {
      "onlyShownOnCommunication": false,
      "hiddenApp": false,
      "watchface": true
    },
    "messageKeys": [
      "SettingAltClockName",
      "SettingAltClockOffset",
      "SettingDisableAutobattery",
      "SettingBluetoothVibe",
      "SettingDisconnectIcon",
      "SettingClockFontId",
      "SettingColorBG",
      "SettingColorSidebar",
      "SettingColorTime",
      "SettingHourlyVibe",
      "SettingLanguageID",
      "SettingShowBatteryPct",
      "SettingShowLeadingZero",
      "SettingSidebarOnLeft",
      "SettingSidebarTextColor",
      "SettingUseLargeFonts",
      "SettingWidget0ID",
      "SettingWidget1ID",
      "SettingWidget2ID"
    ],
    "resources": {
      "media": [
        {
          "file": "images/menuicon.png",
          "menuIcon": true,
          "name": "MENU_ICON",
          "type": "bitmap"
        },
        {
          "file": "fonts/AvenirNextRegular.ffont",
          "name": "AVENIR_REGULAR_FFONT",
          "type": "raw"
        },
        {
          "file": "fonts/AvenirNextDemiBold.ffont",
          "name": "AVENIR_BOLD_FFONT",
          "type": "raw"
        },
        {
          "file": "data/DISCONNECTED.pdc",
          "name": "DISCONNECTED",
          "type": "raw"
        },
        {
          "file": "data/DATE_BG.pdc",
          "name": "DATE_BG",
          "type": "raw"
        },
        {
          "file": "data/BATTERY_CHARGE.pdc",
          "name": "BATTERY_CHARGE",
          "type": "raw"
        },
        {
          "file": "data/BATTERY_BG.pdc",
          "name": "BATTERY_BG",
          "type": "raw"
        }
      ]
    }
  },
  "devDependencies": {
    "pebble-fctx-compiler": "^1.1.0"
  }
}
